<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(traning) -%>">
  <span class='title'>
    <%= traning.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.tranings_traning_path(traning),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_tranings_admin_traning_path(traning),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.tranings_admin_traning_path(traning),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => traning.title),
        :method => :delete %>
  </span>
</li>